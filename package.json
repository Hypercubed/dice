{
  "name": "dice",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --ssl --port 8600",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "watch:test": "ng test",
    "test": "run-s test:*",
    "test:lint": "ng lint",
    "test:unit": "ng test --browsers=Headless --no-watch",
    "deploy": "npm run deploy:main && npm run deploy:version",
    "deploy:main": "ng deploy",
    "deploy:version": "node ./deploy-version.js",
    "lint": "ng lint",
    "fix": "run-s fix:*",
    "fix:lint": "eslint \"src/**/*.{js,ts}\" --ext .ts --fix"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.1.0",
    "@angular/cdk": "^13.1.1",
    "@angular/common": "~13.1.0",
    "@angular/compiler": "~13.1.0",
    "@angular/core": "~13.1.0",
    "@angular/forms": "~13.1.0",
    "@angular/material": "^13.1.1",
    "@angular/platform-browser": "~13.1.0",
    "@angular/platform-browser-dynamic": "~13.1.0",
    "@angular/router": "~13.1.0",
    "crypto-js": "^4.1.1",
    "html5-qrcode": "^2.1.3",
    "ismobilejs": "^1.1.1",
    "qrcode-svg": "^1.1.0",
    "rxjs": "~7.4.0",
    "svgsaver": "^0.9.0",
    "tslib": "^2.3.0",
    "url-safe-base64": "^1.1.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.1.2",
    "@angular-eslint/builder": "13.1.0",
    "@angular-eslint/eslint-plugin": "13.1.0",
    "@angular-eslint/eslint-plugin-template": "13.1.0",
    "@angular-eslint/schematics": "13.1.0",
    "@angular-eslint/template-parser": "13.1.0",
    "@angular/cli": "~13.1.2",
    "@angular/compiler-cli": "~13.1.0",
    "@types/crypto-js": "^4.0.2",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "@types/qrcode-svg": "^1.1.1",
    "@types/semver": "^7.3.9",
    "@types/url-safe-base64": "^1.1.0",
    "@typescript-eslint/eslint-plugin": "5.11.0",
    "@typescript-eslint/parser": "5.11.0",
    "angular-cli-ghpages": "^1.0.0",
    "eslint": "^8.2.0",
    "jasmine-core": "~3.10.0",
    "karma": "^6.3.17",
    "karma-chrome-launcher": "^3.1.1",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "npm-run-all": "^4.1.5",
    "typescript": "~4.5.2"
  }
}